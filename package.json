{
  "name": "form-management-app",
  "version": "1.0.0",
  "private": true,
  "description": "A modern form management system with admin dashboard and user form filling capabilities",
  "author": "Your Organization",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "npm run build && npx serve -s build",
    "test": "react-scripts test --watchAll=false --coverage --passWithNoTests",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx --fix",
    "lint:check": "eslint src --ext .js,.jsx",
    "format": "prettier --write src/**/*.{js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,json,css,md}",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "clean": "rm -rf build node_modules package-lock.json && npm install",
    "prebuild": "npm run lint:check && npm run test",
    "postbuild": "echo 'Build completed successfully!'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "no-debugger": "error",
      "no-unused-vars": "warn",
      "prefer-const": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "your-repository-url"
  },
  "keywords": [
    "react",
    "forms",
    "dashboard",
    "admin",
    "management"
  ]
}
